  "scripts": {
    "import-games": "pnpm ts-node ./scripts/ts/chess/import/games.ts",
    "import-titled": "pnpm ts-node ./scripts/ts/chess/import/titled.ts",
    "format": "prettier --cache --write .",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "printenv": "./scripts/sh/print-env.sh",
    "prebuild": "pnpm run printenv && rimraf .next out public/**/*.js && prisma format && prisma generate",
    "build": "next build",
    "predev": "kill-port 3000 && prisma format && prisma generate",
    "dev": "next dev -p 3000",
    "prestart": "kill-port 3000",
    "start": "next start -p 3000",
    "pretest": "rimraf coverage && prisma format && prisma generate",
    "test": "jest --coverage"
  },
